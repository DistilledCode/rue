dry_run: False

clean_slate: true

max_logs: 9000 # must be > 2 or logs will be deleted and added in a infinite loop

max_saved_ids: 500

new_post_lim: 10

rising_post_lim: 10

min_valid_com_score: 50

min_valid_post_score: 500

max_post_token_len: 20

max_com_char_len: 1000

maturing_time: 3 # hours

min_self_com_score: 20

acc_score_target: 1000

# must be wrapped in square brackets
sleep_time: !!python/object/apply:builtins.range [10, 31, 5] # range(1,2,3)

log_level:
  stream: "info"
  db: "debug"
